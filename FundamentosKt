    println("")
    println("valor de berennice:")
    println("")
    println(bernnadetthy)
    bernnadetthy -= josias
    println(bernnadetthy)*/
    
    // Condições if...else
    // 
    // se...se não
    // 
    // estrutura:
    // 
    // if...else simples
    // 
    // if(condição){
    // ação
    // }else{
    // ação
    // }

    /*var interruptor = true
    
